info.cofh.command.killall.syntax=/cofh killall [部分实体名]
info.cofh.command.tps.syntax=/cofh tps [{o | a | <维度ID>}]
info.cofh.command.tpx.syntax=/cofh tpx [玩家] {(<玩家> | <维度>) | <x> <y> <z> [维度]}
info.cofh.command.version.syntax=/cofh version
info.cofh.command.enchant.syntax=/cofh enchant [玩家] <附魔ID> [等级]
info.cofh.command.clearblocks.syntax=/cofh clearblocks {<用户名> <x范围> <y范围> <z范围> | <x起始点> <y起始点> <z起始点> <x结束点> <y结束点> <z结束点>} [block#meta] [block#meta] ...
info.cofh.command.countblocks.syntax=/cofh countblocks {<用户名> <x范围> <y范围> <z范围> | <x起始点> <y起始点> <z起始点> <x结束点> <y结束点> <z结束点>} [block#meta] [block#meta] ...
info.cofh.command.pregen.syntax=/cofh pregen {<用户名> <x区块范围> <y区块范围> <z区块范围> | <x区块起始点> <y区块起始点> <z区块起始点> <x区块结束点> <y区块结束点> <z区块结束点>}
info.cofh.command.replaceblocks.syntax=/cofh replaceblocks {<用户名> <x范围> <y范围> <z范围> | <x起始点> <y起始点> <z起始点> <x结束点> <y结束点> <z结束点>} <replaceBlock#meta> [block#meta] [block#meta] ...
info.cofh.command.reloadworldgen.syntax=/cofh reloadworldgen
info.cofh.command.unloadchunk.syntax=/cofh unloadchunk

