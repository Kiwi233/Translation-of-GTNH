val I18N_Blood_Magic_Thaumcraft_43 = "\u6cd5\u672f\u8868\u662f\u8840\u9b54\u6cd5\u6dfb\u52a0\u7684\u4e00\u4e2a\u65b9\u5757\uff0c\u7528\u4f5c\u57fa\u672c\u6cd5\u672f\u7cfb\u7edf\u7684\u7126\u70b9\u3002\u5b83\u4e0e\u672a\u7ed1\u5b9a\u6c34\u6676\u548c\u8840\u575b\u4e00\u8d77\u4f7f\u7528\u3002\u5236\u4f5c\u5b8c\u6cd5\u672f\u8868\u540e\uff0c\u5c06\u5176\u653e\u5728\u8840\u575b\u65c1\u8fb9\u3002\u4e4b\u540e\uff0c\u5fc5\u987b\u5728\u6cd5\u672f\u8868\u4e0a\u653e\u7f6e\u4e00\u4e2a\u5934\u9aa8\uff0c\u4ee5\u786e\u5b9a\u65bd\u6cd5\u65f6\u4f1a\u4ea7\u751f\u4ec0\u4e48\u6837\u7684\u6548\u679c\u3002\u9ab7\u9ac5\u5934\u8868\u793a\u653b\u51fb\u6027\u8fdc\u7a0b\u6cd5\u672f\uff0c\u5a01\u745f\u9ab7\u9ac5\u5934\u8868\u793a\u8fdb\u653b\u6027\u8fd1\u6218\uff0c\u50f5\u5c38\u5934\u8868\u793a\u9632\u5fa1\u6cd5\u672f\uff0c\u722c\u884c\u5934\u8868\u793a\u73af\u5883\u6cd5\u672f\u3002\u4e00\u65e6\u9009\u62e9\u4e86\u60f3\u8981\u7684\u6cd5\u672f\u7c7b\u578b\uff0c\u73a9\u5bb6\u5c31\u53ef\u4ee5\u5728\u76f8\u90bb\u7684\u8840\u575b\u4e2d\u653e\u7f6e\u51e0\u4e2a\u7269\u54c1\uff0c\u4ee5\u51c6\u786e\u8868\u793a\u60f3\u8981\u65bd\u653e\u7684\u6cd5\u672f\u3002";
